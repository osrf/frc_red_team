<launch>
  <group ns="diffy">

  <arg name="x" />
  <arg name="y" />

  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
      args="-sdf -x $(arg x) -y $(arg y) -z 0 -model diffy -file $(find frcred_gazebo)/models/diffy/model.sdf "/>
  </group>
</launch>
